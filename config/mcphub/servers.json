{
  "nativeMCPServers": [
    
  ],
  "mcpServers": {
    "sentry": {
      "args": [
        "-y",
        "mcp-remote@latest",
        "https://mcp.sentry.dev/mcp"
      ],
      "autoApprove": [
        "whoami",
        "find_organizations",
        "find_teams",
        "find_projects",
        "find_issues",
        "find_releases",
        "find_tags",
        "get_issue_details",
        "update_issue",
        "find_errors",
        "find_transactions",
        "create_team",
        "create_project",
        "create_dsn",
        "update_project",
        "find_dsns",
        "begin_seer_issue_fix",
        "get_seer_issue_fix_status"
      ],
      "command": "npx"
    },
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "autoApprove": [
        "resolve-library-id",
        "get-library-docs"
      ],
      "disabled": false,
      "command": "npx"
    },
    "git": {
      "args": [
        "mcp-server-git"
      ],
      "autoApprove": [
        "git_status",
        "git_diff_unstaged",
        "git_diff_staged",
        "git_diff",
        "git_commit",
        "git_add",
        "git_reset",
        "git_log",
        "git_create_branch",
        "git_checkout",
        "git_show"
      ],
      "disabled": false,
      "command": "uvx"
    }
  }
}