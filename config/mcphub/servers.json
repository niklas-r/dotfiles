{
  "nativeMCPServers": [
    
  ],
  "mcpServers": {
    "git": {
      "command": "uvx",
      "autoApprove": [
        "git_status",
        "git_diff_unstaged",
        "git_diff_staged",
        "git_diff",
        "git_commit",
        "git_add",
        "git_reset",
        "git_log",
        "git_create_branch",
        "git_checkout",
        "git_show"
      ],
      "args": [
        "mcp-server-git"
      ],
      "disabled": false
    },
    "context7": {
      "command": "npx",
      "autoApprove": [
        "resolve-library-id",
        "get-library-docs"
      ],
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "disabled": false
    }
  }
}